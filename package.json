{
  "name": "coolors-io",
  "version": "1.1.0",
  "description": "Parse a coolors URL into an array of hex strings",
  "main": "lib",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "example": "nodemon lib/example.js",
    "lint": "tslint -p tsconfig.json",
    "preadd": "npm run lint",
    "add": "git add .",
    "precommit": "npm run add",
    "commit": "git commit -am $npm_config_msg",
    "version": "npm version patch -m $npm_config_msg",
    "update": "npm run commit",
    "publish": "npm run commit && git push origin master && npm publish"
  },
  "files": [
    "lib/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delesslin/coolors-io.git"
  },
  "keywords": [
    "coolors",
    "colors",
    "hexadecimal",
    "hex",
    "js",
    "javascript",
    "url"
  ],
  "author": "delesslin <inc@delesslin.com> (http://delesslin.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delesslin/coolors-io/issues"
  },
  "homepage": "https://github.com/delesslin/coolors-io#readme",
  "devDependencies": {
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  }
}
